=render "searchform"
.row
  .large-6.columns
    - @apiresponseleft["JobSearchResult"].each do |jobresult|
      .row.job-row
        .large-6.columns
          = link_to jobresult["JobTitle"], jobresult["JobDetailsURL"]
        .large-2.columns
          =jobresult["Company"]
        .large-2.columns
          =jobresult["Location"]
        .large-2.columns
          =jobresult["PostedDate"]
